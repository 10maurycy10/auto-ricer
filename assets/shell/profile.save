export EDITOR=nano
export PAGER=less
export TERMINAL=st
export MOZ_ENABLE_WAYLAND=1
export TERMINAL=alacritty
export PATH="$HOME/.local/bin/:$HOME/.local/usr/local/bin/:/usr/local/bin:/bin:/usr/bin:$HOME/go/bin"
export LD_LIBRARY_PATH="$HOME/.local/lib:/lib:/usr/lib:/usr/local/lib"
export SHELL="/usr/bin/zsh"

# disable ^S ^Q
stty -ixon

# allow huge history
export HISTFILESIZE=
export HISTSIZE=
export HISTTIMEFORMAT="[%F %T] "
